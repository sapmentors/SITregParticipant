{
	"_version": "1.3.0",
	"sap.app": {
		"_version": "1.3.0",
		"id": "com.sap.sapmentors.sitreg.registration",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "/destinations/HANAMDC/com/sap/sapmentors/sitreg/odataparticipant/service.xsodata/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		},
		"sourceTemplate": {
			"id": "sap.ui.ui5-template-plugin.2masterdetail",
			"version": "1.36.2"
		},
		"crossNavigation": {
			"inbounds": {
				"intent1": {
					"signature": {
						"parameters": {},
						"additionalParameters": "allowed"
					},
					"semanticObject": "SAPCommunityEvent",
					"action": "register",
					"title": "Register",
					"subTitle": "SAP Community Event",
					"info": "Participants of SAP Community Events can use this app to register for Event and edit their registration"
				}
			}
		}
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://detail-view",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_belize",
			"sap_bluecrystal",
			"sap_hcb"
		]
	},
	"sap.ui5": {
		"_version": "1.2.0",
		"rootView": {
			"viewName": "com.sap.sapmentors.sitreg.registration.view.App",
			"type": "XML",
			"id": "app"
		},
		"resourceRoots": {
			"com.penninkhof.controls": "./control"
		},
		"dependencies": {
			"minUI5Version": "1.36.0",
			"libs": {
				"sap.ui.core": {
					"minVersion": "1.36.0"
				},
				"sap.m": {
					"minVersion": "1.36.0"
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.sap.sapmentors.sitreg.registration.i18n.i18n"
				}
			},
			"currentUser": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "/services/userapi/currentUser"
			},
			"": {
				"dataSource": "mainService",
				"settings": {
					"metadataUrlParams": {
						"sap-documentation": "heading"
					}
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "com.sap.sapmentors.sitreg.registration.view",
				"controlId": "idAppControl",
				"controlAggregation": "detailPages",
				"bypassed": {
					"target": [
						"master",
						"notFound"
					]
				},
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "master",
					"target": [
						"object",
						"master"
					]
				},
				{
					"pattern": "Events/{objectId}",
					"name": "object",
					"target": [
						"master",
						"object"
					]
				},
				{
					"pattern": "Events/{objectId}/RegisterParticipiant",
					"name": "register",
					"target": [
						"register"
					]
				},
				{
					"pattern": "Events/{objectId}/Ticket",
					"name": "ticket",
					"target": [
						"ticket"
					]
				},
				{
					"name": "registerAsOrganizer",
					"pattern": "RegisterAsOrganizer",
					"greedy": false,
					"target": [
						"registerAsOrganizer"
					]
				}
			],
			"targets": {
				"master": {
					"viewName": "Master",
					"viewLevel": 1,
					"viewId": "master",
					"controlAggregation": "masterPages"
				},
				"object": {
					"viewName": "Detail",
					"viewId": "detail",
					"viewLevel": 2
				},
				"register": {
					"viewName": "CreateEntity",
					"viewId": "createentity",
					"viewLevel": 3
				},
				"ticket": {
					"viewName": "Ticket",
					"viewId": "ticket",
					"viewLevel": 3
				},
				"detailObjectNotFound": {
					"viewName": "DetailObjectNotFound",
					"viewId": "detailObjectNotFound"
				},
				"detailNoObjectsAvailable": {
					"viewName": "DetailNoObjectsAvailable",
					"viewId": "detailNoObjectsAvailable"
				},
				"notFound": {
					"viewName": "NotFound",
					"viewId": "notFound"
				},
				"registerAsOrganizer": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "RegisterAsOrganizer",
					"viewId": "registerasorganizer",
					"viewLevel": 3
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}